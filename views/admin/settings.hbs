			<main class="content">
				<div class="container-fluid p-0">

					<h1 class="h3 mb-3"><strong>Banned</strong> Employers</h1>

					<div class="row">
						<div class="col-12 col-lg-12 col-xxl-12 d-flex">
							<div class="card flex-fill">
								<table class="table table-hover my-0">
									<thead>
										<tr>
											<th>Name</th>
											<th class="d-none d-xl-table-cell">Email</th>
											<th></th>
											<th class="d-none d-md-table-cell">Actions</th>
										</tr>
									</thead>
									<tbody>
										{{#each bannedemployee}}
										<tr>
											<td class="text-success"><b>{{this.name}}</b></td>
											<td class="d-none d-xl-table-cell">{{this.email}}</td>
											<td><a href="/admin/viewjobs?id={{this._id}}" class="text-primary">View jobs</a></td>
											<td class="d-none d-md-table-cell">
												<a href="/admin/unbanemployer?id={{this._id}}" class="p-2 bg-warning m-1" data-toggle="tooltip" data-placement="top" >UnBan Employer<i class="align-middle" data-feather="eye" onclick="return confirm('are you sure to UnBan {{this.name}}?')"></i></a>
											</td>
										</tr>
										{{/each}}
										
									</tbody>
								</table>
							</div>
						</div>				
					</div>
				</div>


				<h1 class="h3 mb-3 mt-4 pt-4"><strong>Banned</strong> Users</h1>

					<div class="row">
						<div class="col-12 col-lg-12 col-xxl-12 d-flex">
							<div class="card flex-fill">
								<table class="table table-hover my-0">
									<thead>
										<tr>
											<th>Name</th>
											<th class="d-none d-xl-table-cell">Email</th>
											<th class="d-none d-md-table-cell">Actions</th>
										</tr>
									</thead>
									<tbody>
										{{#each banneduser}}
										<tr>
											<td class="text-success"><b>{{this.Name}}</b></td>
											<td class="d-none d-xl-table-cell">{{this.Email}}</td>
											<td class="d-none d-md-table-cell">
										<a href="/admin/unbanuser?id={{this._id}}" class="p-2 bg-warning m-1" data-toggle="tooltip" data-placement="top">UnBan User<i class="align-middle" data-feather="eye" onclick="return confirm('are you sure to UnBan {{this.name}}?')"></i></a>
											</td>
										</tr>
										{{/each}}
										
									</tbody>
								</table>
							</div>
						</div>				
					</div>
			</main>

			
		</div>
	</div>